@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />

<div class="row">
    <div class='col-md-12'>
        <div class="form-group">
            <input type='text' class="form-control" id='datetimepicker' />
        </div>
    </div>
</div>


<div id="fetch">
    <input class="no-clear typeahead form-control" type="text" placeholder="Products">
</div>


<div class="row">
    <div class="col-md-12">
        <table id="foodItemTable2" class="table table-bordered table-striped table-hover">
            <thead>
                <tr>
                    <th class="col-md-4">Food product</th>
                    <th class="col-md-4">Quantity</th>
                    <th class="col-md-4">Links</th>
                </tr>
            </thead>
            <tbody id="foodItemTableBody"></tbody>
        </table>
    </div>
</div>

<script src="@Url.Content("~/Scripts/typeahead.bundle.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/typeahead.jquery.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/trackablesfoodlog.js")" type="text/javascript"></script>
