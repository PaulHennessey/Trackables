@model  Trackables.Models.FoodLogViewModel
@using Trackables.Models

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />
<h1>index 2</h1>

<br />

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

    <div class="row">
        <div class='col-md-12'>
            <div class="form-group">
                <input type='text' class="form-control" id='datetimepicker' />
            </div>
        </div>
    </div>

    <div id="fetch">
        <input class="no-clear typeahead form-control" type="text" placeholder="Products">
    </div>

    <br />

    <div class="row">
        <div class="col-md-12">
            @Html.Partial("FoodItemTable")
        </div>
    </div>
}

<script src="@Url.Content("~/Scripts/typeahead.bundle.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/typeahead.jquery.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/trackables-typeahead.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/trackables-foodlog.js")" type="text/javascript"></script>


